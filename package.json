{
  "name": "@onkernel/ai-sdk",
  "version": "0.1.0",
  "description": "Vercel AI SDK-compatible tools for Onkernel Kernel SDK (Playwright execution).",
  "keywords": [
    "vercel",
    "ai",
    "sdk",
    "tools",
    "onkernel",
    "playwright",
    "browser",
    "agents",
    "anthropic",
    "computer-use"
  ],
  "license": "MIT",
  "author": "Onkernel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onkernel/ai-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/onkernel/ai-sdk/issues"
  },
  "homepage": "https://github.com/onkernel/ai-sdk#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@onkernel/sdk": "*",
    "ai": "^5"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^1.1.15",
    "@ai-sdk/openai": "latest",
    "@onkernel/sdk": "latest",
    "@types/node": "^24.10.1",
    "ai": "^5",
    "tsup": "latest",
    "tsx": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "^3.0.19",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
