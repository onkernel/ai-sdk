{
  "name": "@onkernel/ai-sdk",
  "version": "0.1.0",
  "description": "Vercel AI SDK-compatible tools for Onkernel Kernel SDK (Playwright execution).",
  "keywords": ["vercel", "ai", "sdk", "tools", "onkernel", "playwright", "browser", "agents"],
  "license": "MIT",
  "author": "Onkernel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onkernel/ai-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/onkernel/ai-sdk/issues"
  },
  "homepage": "https://github.com/onkernel/ai-sdk#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "ai": "*",
    "@onkernel/sdk": "*"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "tsup": "^8.0.0",
    "ai": "^4.0.0"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
